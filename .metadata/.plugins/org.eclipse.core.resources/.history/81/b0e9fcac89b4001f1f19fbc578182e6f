package com.tka.strings;

public class Temp {
	public static void main(String[] args) {

		String name = "Ajay";

//		1. String Length
		int length = name.length();
		System.out.println(length);

//		Print the characters

		for (int i = 0; i < name.length(); i++) {
			System.out.println(name.charAt(i));
		}

	}
}
